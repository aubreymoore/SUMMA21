  title = {Studies on {{Rhabdionvirus}} Oryctes: {{I}}. {{Effect}} on {{Larvae}} of {{Oryctes}} Rhinoceros and Inactivation of the Virus},
  author = {Zelazny, Bernhard},
  date = {1972},
