@article{zelazny_studies_1973,
  title = {Studies on {{Rhabdionvirus}} Oryctes: {{III}}. {{Incidence}} in the {{Oryctes}} Rhinoceros Population of {{Western Samoa}}},
  shorttitle = {Studies on {{Rhabdionvirus}} Oryctes},
  author = {Zelazny, B.},
  date = {1973-11-01},
  journaltitle = {Journal of Invertebrate Pathology},
  shortjournal = {Journal of Invertebrate Pathology},
  volume = {22},
  pages = {359--363},
  issn = {0022-2011},
  doi = {10.1016/0022-2011(73)90164-X},
  url = {http://www.sciencedirect.com/science/article/pii/002220117390164X},
  urldate = {2020-02-19},
  abstract = {The viral pathogen Rhabdionvirus oryctes [ROV] was found to be widespread in the population of Oryctes rhinoceros in Western Samoa. About 3\% of the larvae and 35\% of the adults were infected with ROV. Larval breeding sites more frequently contained ROV-infected insects if the sites were occupied by two broods instead of one and if there was a high density of breeding in the area. Females infected with ROV visited the breeding sites less frequently for egg-laying than healthy females. The male adults, as they increased in age, were found to be more frequently infected with ROV, but the young female adults were more frequently infected than very young or old females. The ROV-infections in adults apparently play a more important role in suppressing the O. rhinoceros population than those of larvae.},
  file = {/home/aubrey/Zotero/storage/E89HS8RI/002220117390164X.html},
  langid = {english},
  number = {3}
}

@article{zelazny_studies_1973-1,
  title = {Studies on {{Rhabdionvirus}} Oryctes: {{II}}. {{Effect}} on Adults of {{Oryctes}} Rhinoceros},
  shorttitle = {Studies on {{Rhabdionvirus}} Oryctes},
  author = {Zelazny, B.},
  date = {1973-07-01},
  journaltitle = {Journal of Invertebrate Pathology},
  shortjournal = {Journal of Invertebrate Pathology},
  volume = {22},
  pages = {122--126},
  issn = {0022-2011},
  doi = {10.1016/0022-2011(73)90020-7},
  url = {http://www.sciencedirect.com/science/article/pii/0022201173900207},
  urldate = {2020-02-29},
  abstract = {Rhabdionvirus oryctes multiplies in adults of Oryctes rhinoceros. Adults and larvae of O. rhinoceros showed no significant difference in their susceptibility to the virus when it was inoculated into the hemocoel. Adults, collected from the natural population in Western Samoa, were found to be frequently infected with Rhabdionvirus. In the laboratory the infected adults had a shorter life-span and laid fewer eggs than the noninfected ones. Infected adults excreted virus material into the surrounding medium. This suggests a possible means of transmission of the virus in the field.},
  file = {/home/aubrey/Zotero/storage/8UTTNN5S/0022201173900207.html},
  langid = {english},
  number = {1}
}


