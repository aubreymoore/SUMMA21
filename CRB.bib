@article{r.pradeep_kuma_isolation_2019,
  title = {Isolation, Characterization and Mode of Action of a Larvicidal Compound, 22-Hydroxyhopane from {{Adiantum}} Latifolium {{Lam}}. against {{Oryctes}} Rhinoceros {{Linn}}.},
