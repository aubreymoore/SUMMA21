@article{dyah_rini_indriyanti_density_2017,
  title = {{{DENSITY AND COMPOSITION OF Oryctes Rhinoceros}} ({{COLEOPTERA}}: {{SCARABAEIDAE}}) {{STADIA}}  {{IN FIELD}}},
